{
  "version": 2,
  "builds": [
    {
      "src": "dist/charioft/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/charioft/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*?)\\.(js|css|ico|svg|png|jpg|jpeg|gif|woff|woff2|ttf)",
      "dest": "/dist/charioft/browser/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/charioft/server/main.js"
    }
  ]
}